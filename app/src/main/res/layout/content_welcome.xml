<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context="com.example.rucha.loginact.WelcomeActivity"
    tools:showIn="@layout/app_bar_main2">

    <TextView
        android:id="@+id/lblWelcome"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="150dp"

        />

    <ImageView
        android:id="@+id/dispImg"
        android:layout_width="325dp"
        android:layout_height="325dp"
        android:layout_centerHorizontal="true"
        tools:layout_editor_absoluteX="30dp"
        tools:layout_editor_absoluteY="0dp"
        tools:ignore="MissingConstraints" />


    <Button
        android:id="@+id/btnListView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/dispImg"
        android:layout_below="@+id/dispImg"
        android:text="List View"
        tools:layout_editor_absoluteX="249dp"
        tools:layout_editor_absoluteY="354dp"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/btnImagefromGallery"
        android:layout_width="99dp"
        android:layout_height="55dp"
        android:layout_alignBottom="@+id/btnListView"
        android:layout_toEndOf="@+id/btnWofTitleList"
        android:text="Upload Image from Gallery"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="347dp"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/btnWofTitleList"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/btnListView"
        android:layout_toEndOf="@+id/btnListView"
        android:text="W/o Fragment"
        tools:layout_editor_absoluteX="110dp"
        tools:layout_editor_absoluteY="354dp"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/btnWfQuotes"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/btnListView"
        android:layout_below="@+id/btnListView"
        android:text="W Fragment"
        tools:layout_editor_absoluteX="136dp"
        tools:layout_editor_absoluteY="457dp"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/btnDatabase"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/btnWfQuotes"
        android:layout_toEndOf="@+id/btnWfQuotes"
        android:text="Database"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="402dp"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/btnMenu"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/btnDatabase"
        android:layout_toEndOf="@+id/btnDatabase"
        android:text="Menu"
        tools:layout_editor_absoluteX="113dp"
        tools:layout_editor_absoluteY="402dp"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/btnMusic"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/btnMenu"
        android:layout_toEndOf="@+id/btnMenu"
        android:text="Music"
        tools:layout_editor_absoluteX="295dp"
        tools:layout_editor_absoluteY="402dp"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/btnDateTime"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/btnDatabase"
        android:layout_toEndOf="@+id/imgbtnUpload"
        android:text="DateTime"
        tools:layout_editor_absoluteX="204dp"
        tools:layout_editor_absoluteY="402dp"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/btnGetLocation"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/btnWfQuotes"
        android:layout_toStartOf="@+id/btnDateTime"
        android:text="Location"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="204dp"
        tools:layout_editor_absoluteY="402dp" />

    <ImageButton
        android:id="@+id/imgBack"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_alignParentStart="true"
        android:layout_weight="1"
        app:srcCompat="@android:drawable/ic_menu_revert"
        tools:layout_editor_absoluteX="64dp"
        tools:layout_editor_absoluteY="457dp"
        tools:ignore="MissingConstraints" />

    <ImageButton
        android:id="@+id/imgbtnUpload"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/imgBack"
        android:layout_toEndOf="@+id/imgBack"
        android:layout_weight="1"
        app:srcCompat="@android:drawable/ic_menu_upload"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="457dp"
        tools:ignore="MissingConstraints" />




</RelativeLayout>


